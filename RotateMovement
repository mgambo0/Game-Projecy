using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class RotateMovement : MonoBehaviour
{
    // This will initiate the code within the first possible frame.
    void Start()
    {
        Debug.Log("Hello Jimmy!");
    }

    // This will make the code update within every frame.
    void Update()
    {
        transform.Rotate(0, .25f, 0);
    }
}
